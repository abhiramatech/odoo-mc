<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="portal_back_in_edit_mode" name="Back to edit mode">
        <div t-ignore="true" class="text-center">
            <t t-if="custom_html" t-out="custom_html"/>
            <t t-else="">This is a preview of report POS.</t>
            <a t-att-href="backend_url" 
                class="alert-link"><i class="oi oi-arrow-right me-1"/>Back to edit mode</a>
        </div>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </template>

</odoo>